{
    "extends": ["standard", "Plugin:vue/recommended", "Plugin:tailwindcss/recommended", "prettier"],
    "rules": {
        "max-len": [2, 250],
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 1,
                "maxEOF": 1
            }
        ],
        "object-curly-newline": 0
    }
}
